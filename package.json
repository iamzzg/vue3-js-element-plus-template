{
  "name": "vue-demo",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{scr,mock}/**/*.{vue,js,jsx,ts,tsx}\" --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,css,less,scss,postcss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "svgo": "svgo -f ./src/assets/svg -o ./src/assets/icons --config=svgo.config.js"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "axios": "^0.27.2",
    "element-plus": "^2.2.12",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.16",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.29",
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.5.1",
    "rollup": "^2.77.2",
    "sass": "^1.54.3",
    "stylelint": "14",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "svgo": "^2.8.0",
    "unplugin-auto-import": "^0.11.0",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.0",
    "vite": "^3.0.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.8.7",
    "windicss": "^3.5.6"
  }
}
